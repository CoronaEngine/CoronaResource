---
Checks: >
  google-*,
  -google-build-using-namespace,
  -google-readability-todo

CheckOptions:
  - key:             google-readability-identifier-naming.ClassCase
    value:           CamelCase
  - key:             google-readability-identifier-naming.StructCase
    value:           CamelCase
  - key:             google-readability-identifier-naming.InterfaceCase
    value:           CamelCase
  - key:             google-readability-identifier-naming.EnumCase
    value:           CamelCase
  - key:             google-readability-identifier-naming.FunctionCase
    value:           snake_case
  - key:             google-readability-identifier-naming.VariableCase
    value:           snake_case
  - key:             google-readability-identifier-naming.MemberCase
    value:           snake_case_
  - key:             google-readability-identifier-naming.EnumConstantCase
    value:           kCamelCase
  - key:             google-readability-identifier-naming.ConstantCase
    value:           kCamelCase
  - key:             google-readability-identifier-naming.MemberConstantCase
    value:           kCamelCase
  - key:             google-readability-identifier-naming.GlobalConstantCase
    value:           kCamelCase
  - key:             google-readability-identifier-naming.PrivateMemberSuffix
    value:           _
  - key:             google-readability-identifier-naming.IgnoreMainLikeFunctions
    value:           1
...
